  pod 'KakaoSDKUser'  # 사용자 관리
  pod 'KakaoSDKTalk'  # 친구, 메시지(카카오톡)
  pod 'KakaoSDKLink'  # 메시지(카카오링크)
  pod 'TTGTagCollectionView'
end
