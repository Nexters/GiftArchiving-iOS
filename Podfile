# platform :ios, '9.0'

target 'Gift.zip-iOS' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Gift.zip-iOS
  pod 'Alamofire'
  pod 'Kingfisher'
  pod 'SnapKit'
  pod 'Then'
  pod 'RxSwift', '6.0.0'
  pod 'RxCocoa', '6.0.0'
  pod 'KakaoSDKCommon'  # 필수 요소를 담은 공통 모듈
